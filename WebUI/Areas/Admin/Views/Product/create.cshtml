


<form method="post" enctype="multipart/form-data">
    <input type="text" name="ProductName"  placeholder="Enter Product Name in 
     Az"/>
    <br />
    <input type="text" name="ProductName" placeholder="Enter Product Name in
     En" />
    <br />
    <input type="text" name="ProductName" placeholder="Enter Product Name in
     Ru" />
    <br />
    <input type="text" name="Description" placeholder="Enter Description in
     Az" />
    <br />
    <input type="text" name="Description" placeholder="Enter Descriptionin
     En" />
    <br />
    <input type="text" name="Description" placeholder="Enter Description in
     Ru" />
    <br />
    <input type="number" name="Price" />
    <input type="number" name="Discount" />
    <input type="number" name="Quantity" />
    <br />
    <select name="CategoryId" asp-items="ViewBag.Categories"></select>
    <br />
    <input id="productPicture" multiple type="file" accept="image/*" name="photoUrls" />
    <br />
     <div class="card">
         <div id="productList" class="row">
             <div class="col-2">
                 <div class="card p-3">
                    <img class="img-fluid" src="data:image/jpeg;base64," />
                 </div>
             </div>
         </div>
     </div>
    <br />
    <input type="submit" value="Create" />
</form>

@section Scripts{
    <script>

        const productPicture = document.getElementById("productPicture");
        const productList = document.getElementById("productList");


        productPicture.addEventListener('change', function(e){

            console.log(this.files)

            var datas = new FormData();
            
            for(let i = 0; i < this.files.length; i++){
                datas.append("photoUrls", productPicture.files[i]);
               
            }

                $.ajax({
                    url: '/Picture/UploadPictures',
                    type:  "POST",
                    dataType: 'json',
                    contentType: false,
                    processData: false,
                    data: datas,
                    success: function(e){
                        console.log(e);
                        CreateCard(e);
                    }
            })

        })
        function CreateCard(photo) {
            for (let i = 0; i < photo.length; i++) {
                productList.innerHTML +=
                    '
                    < div class="col-2" >
                        <div class="card p-3" >
                            <img class="img-fluid" src = "${photo[i]}" name = "${photo[i]}  />
                                < input type = "hidden" value="${photo[i]}" name="PhotoUrls" />
                                    </div>
                                    < /div>            
                '
            }
            
        }

        

    </script>
}